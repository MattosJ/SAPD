{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src/controllers", "src/services", "src/repositories"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/minami"
  }
}
